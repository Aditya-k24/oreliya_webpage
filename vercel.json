{
  "outputDirectory": "apps/web/dist",
  "framework": "vite",
  "public": true,
  "headers": [
    {
      "source": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp|mp4|webm))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.).*)$",
      "destination": "/index.html"
    }
  ]
}
