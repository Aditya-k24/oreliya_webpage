    "@types/form-data": "^2.5.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/stripe": "^8.0.417",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.4",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.0.3",
    "stripe": "^18.5.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
