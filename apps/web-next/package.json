    "@testing-library/user-event": "^14.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^1.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jsdom": "^23.0.0",
