{
  "name": "web-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:production": "NODE_ENV=production prisma generate && NODE_ENV=production next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "vercel-build": "prisma generate && next build",
    "precommit": "pnpm run lint && pnpm run type-check",
    "prebuild": "pnpm run lint && pnpm run type-check",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out node_modules/.cache",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "postinstall": "pnpm run db:generate"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@supabase/storage-js": "^2.12.2",
    "@supabase/supabase-js": "^2.58.0",
    "@types/form-data": "^2.5.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/stripe": "^8.0.417",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.4",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.0.3",
    "stripe": "^18.5.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jsdom": "^23.0.0",
    "prisma": "^6.11.1",
    "typescript": "^5"
  }
}
